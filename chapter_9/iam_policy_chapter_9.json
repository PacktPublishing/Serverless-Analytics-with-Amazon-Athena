{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "glue:BatchCreatePartition",
                "glue:DeleteDatabase",
                "glue:GetPartitions",
                "glue:UpdateTable",
                "s3:ListBucketVersions",
                "glue:DeleteTable",
                "s3:CreateBucket",
                "s3:ListBucket",
                "athena:UpdateWorkGroup",
                "athena:GetWorkGroup",
                "iam:PassRole",
                "s3:AbortMultipartUpload",
                "glue:CreatePartition",
                "s3:DeleteObject",
                "glue:UpdatePartition",
                "glue:UpdateDatabase",
                "s3:ListBucketMultipartUploads",
                "glue:CreateTable",
                "glue:GetTables",
                "athena:DeleteWorkGroup",
                "glue:BatchGetPartition",
                "glue:GetDatabases",
                "athena:CreateWorkGroup",
                "glue:GetTable",
                "s3:ListMultipartUploadParts",
                "glue:GetDatabase",
                "s3:PutObject",
                "s3:GetObject",
                "glue:GetPartition",
                "lambda:AddPermission",
                "glue:CreateDatabase",
                "glue:DeletePartition",
                "lambda:RemovePermission"
            ],
            "Resource": [
                "arn:aws:glue:*:*:table/*/*",
                "arn:aws:glue:*:*:database/packt_serverless_analytics",
                "arn:aws:glue:*:*:catalog",
                "arn:aws:athena:*:*:workgroup/primary",
                "arn:aws:athena:*:*:workgroup/packt-*",
                "arn:aws:athena:*:<YOUR_ACCOUNT_NUMBER>:datacatalog/*",
                "arn:aws:lambda:*:<YOUR_ACCOUNT_NUMBER>:function:packt-serverless-analytics-*",
                "arn:aws:iam::*:role/packt-serverless-analytics-*",
                "arn:aws:s3:::<YOUR_S3_BUCKET>/*",
                "arn:aws:s3:::<YOUR_S3_BUCKET>"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "athena:StartQueryExecution",
                "lambda:ListVersionsByFunction",
                "athena:GetQueryResults",
                "lambda:InvokeAsync",
                "lambda:GetFunctionConfiguration",
                "athena:DeleteNamedQuery",
                "athena:GetNamedQuery",
                "s3:GetBucketPolicy",
                "athena:ListQueryExecutions",
                "lambda:PutFunctionConcurrency",
                "lambda:ListProvisionedConcurrencyConfigs",
                "lambda:ListTags",
                "athena:StopQueryExecution",
                "lambda:PutFunctionEventInvokeConfig",
                "lambda:DeleteFunctionEventInvokeConfig",
                "lambda:DeleteFunction",
                "lambda:GetAlias",
                "s3:GetBucketPolicyStatus",
                "lambda:UpdateFunctionEventInvokeConfig",
                "athena:GetQueryResultsStream",
                "lambda:UpdateEventSourceMapping",
                "lambda:GetEventSourceMapping",
                "lambda:InvokeFunction",
                "lambda:GetFunction",
                "lambda:ListAliases",
                "lambda:UpdateFunctionConfiguration",
                "s3:GetBucketNotification",
                "s3:DeleteBucketPolicy",
                "athena:ListNamedQueries",
                "lambda:UpdateAlias",
                "lambda:UpdateFunctionCode",
                "s3:PutBucketNotification",
                "athena:CreateNamedQuery",
                "lambda:ListFunctionEventInvokeConfigs",
                "lambda:GetFunctionConcurrency",
                "lambda:GetFunctionEventInvokeConfig",
                "athena:GetQueryExecution",
                "lambda:DeleteAlias",
                "s3:PutBucketPolicy",
                "lambda:PublishVersion",
                "athena:BatchGetNamedQuery",
                "lambda:DeleteFunctionConcurrency",
                "lambda:DeleteEventSourceMapping",
                "athena:BatchGetQueryExecution",
                "lambda:GetPolicy",
                "lambda:CreateAlias"
            ],
            "Resource": [
                "arn:aws:athena:*:*:workgroup/primary",
                "arn:aws:athena:*:*:workgroup/packt-*",
                "arn:aws:athena:*:<YOUR_ACCOUNT_NUMBER>:datacatalog/*",
                "arn:aws:lambda:*:<YOUR_ACCOUNT_NUMBER>:event-source-mapping:*",
                "arn:aws:lambda:*:<YOUR_ACCOUNT_NUMBER>:function:packt-serverless-analytics-*",
                "arn:aws:s3:::<YOUR_S3_BUCKET>"
            ]
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "athena:ListEngineVersions",
                "lambda:ListFunctions",
                "s3:ListJobs",
                "athena:GetNamespace",
                "iam:ListRoles",
                "lambda:GetAccountSettings",
                "athena:ListWorkGroups",
                "lambda:CreateEventSourceMapping",
                "athena:GetCatalogs",
                "athena:ListTagsForResource",
                "athena:GetNamespaces",
                "s3:ListAllMyBuckets",
                "cloudwatch:*",
                "athena:GetExecutionEngine",
                "kms:ListAliases",
                "lambda:ListEventSourceMappings",
                "cloudshell:CreateEnvironment",
                "athena:GetExecutionEngines",
                "athena:GetTables",
                "athena:GetTable"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": "s3:GetBucketLocation",
            "Resource": "arn:aws:s3:::*"
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": "cloudshell:*",
            "Resource": "arn:aws:cloudshell:*:*:environment/*"
        },
        {
            "Sid": "VisualEditor5",
            "Effect": "Allow",
            "Action": "logs:*",
            "Resource": "arn:aws:logs:*:*:*"
        }
    ]
}